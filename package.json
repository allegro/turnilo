    "js-yaml": "3.10.0",
    "lz-string": "1.4.4",
    "marked": "0.4.0",
    "moment": "2.20.1",
    "moment-timezone": "0.5.14",
    "node-spawn-server": "1.0.1",
    "nopt": "4.0.1",
    "numeral": "2.0.6",
    "plywood": "0.19.2",
    "plywood-base-api": "0.2.8",
    "plywood-druid-requester": "2.5.0",
    "q": "1.5.1",
